@model IEnumerable<Restaurant.Models.TransactionContactU>

@{
    ViewData["Title"] = "Index";
}


<div class="pagetitle">

    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Home</a></li>
            <li class="breadcrumb-item"> <a asp-action="Index" asp-controller="TransactionContactU">Transaction Contact Us</a></li>
          
        </ol>
    </nav>
</div>

<section class="section">
    <div class="row">
        <div class="col-lg-12">

            <div class="card">
                <div class="card-body">
                    <h5 class="card-title"></h5>
                    <table class="table datatable">
    <thead>
        <tr>
            <th>
                                    FullName
            </th>
            <th>
                                    Emai
            </th>
            <th>
                                    Subject
            </th>
            <th>
                                    Message
            </th>
           
            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TransactionContactUsFullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TransactionContactUsEmail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TransactionContactUsSubject)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TransactionContactUsMessage)
            </td>
            
        </tr>
}
    </tbody>
</table>
                </div>
            </div>

        </div>
    </div>
</section>